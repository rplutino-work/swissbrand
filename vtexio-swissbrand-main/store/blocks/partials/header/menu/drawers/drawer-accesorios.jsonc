{
    "drawer#accesorios": {
        "props": {
            "blockClass": ["mobile-menu", "secondary-tab"]
        },

        "blocks": ["drawer-trigger#accesorios"],
        "children": ["menu#accesorios"]
    },

    "drawer-trigger#accesorios": {
