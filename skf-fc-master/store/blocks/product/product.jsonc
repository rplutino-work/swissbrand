"condition-layout.product#tab-layout-pdp": {
  "title": "Opciones",
  "props": {
    "conditions": [
       {
         "subject": "categoryTree",
         "arguments": {
             "id": "1"
         }
       }
    ],
    "Then": "tab-layout#home",
    "Else": "condition-layout.product#tab-layout-pdp-merchandasing"
  }
},
"condition-layout.product#tab-layout-pdp-merchandasing": {
  "title": "Opciones",
  "props": {
    "conditions": [
       {
         "subject": "categoryTree",
         "arguments": {
             "id": "2"
         }
       }
    ],
    "Then": "tab-layout#home-3",
    "Else": "tab-layout#home-2"
  }
},
"tab-layout#home-3": {
  "children": [
    "tab-list#home-3",
    "tab-content#home-3"
  ],
  "props": {
    "blockClass": "home",
    "defaultActiveTabId": "home1"
  }
},
"tab-layout#home": {
    "children": [
      "tab-list#home",
      "tab-content#home"
    ],
    "props": {
      "blockClass": "home",
      "defaultActiveTabId": "home1"
    }
  },
  "tab-list#home": {
    "children": [
      "tab-list.item#home1",
      "tab-list.item#home3"
    ]
  },
  "tab-list#home-3": {
    "children": [
      "tab-list.item#home1"
    ]
  },
  "tab-layout#home-2": {
    "children": [
      "tab-list#home-2",
      "tab-content#home-2"
    ],
    "props": {
      "blockClass": "home",
      "defaultActiveTabId": "home2"
    }
  },
  "tab-list#home-2": {
    "children": [
      "tab-list.item#home2",
      "tab-list.item#home1",
      "tab-list.item#home3"
