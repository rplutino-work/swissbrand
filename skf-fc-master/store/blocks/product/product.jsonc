  "toggle-layout#aplications": {
    "title": "Descargar PDF",
    "children": [
      "condition-layout.product#aplications"
      ]
  },

  "condition-layout.product#aplications": {
    "title": "Opciones",
    "props": {
      "conditions": [
         {
           "subject": "categoryTree",
           "arguments": {
               "id": "1"
           }
         }
      ],
      "Then": "flex-layout.row#aplications",
      "Else": "link#aplications"
    }
  },

  "flex-layout.row#aplications": {
    "children": ["link#download"],
    "props": {
      "blockClass": ["aplicaciones-download"],
      "colSizing": "auto"
    }
  },

  "link#aplications": {
    "title": "Link aplicaciones",
    "props": {
      "href": "#aplicaciones",
      "label": "Aplicaciones",
      "blockClass": "aplications-btn"
    }
  },

  "link#download": {
    "title": "Link PDF",
    "props": {
      "href": "#",
      "label": "Descargar PDF",
      "blockClass": "download-btn",
      "target": "_blank"
    }
  },

