  "tab-list.item#home3": {
    "props": {
      "tabId": "home3",
      "label": "Información técnica"
    }
  },
  "tab-content#home": {
    "children": [
      "tab-content.item#home1",
      "tab-content.item#home3"
    ]
  },
  "tab-content#home-3": {
    "children": [
      "tab-content.item#home1"
    ]
  },
  "tab-content#home-2": {
    "children": [
      "tab-content.item#home1",
      "tab-content.item#home2",
      "tab-content.item#home3"
    ]
  },
  "tab-content.item#home1": {
