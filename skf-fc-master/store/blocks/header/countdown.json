{
    "toggle-layout#countdown-site-desktop": {
        "title": "Contador sitio desktop",
        "children": [
            "flex-layout.row#countdown-site-desktop"
          ]
      },
    
      "flex-layout.row#countdown-site-desktop": {
        "title": "Configuraciones",
        "children": [
          "disposition-layout#countdown-site-desktop"
        ],
        "props": {
          "blockClass": ["countdown-wrapper", "countdown-wrapper-site-desktop"],
          "colSizing": "equal"
        }
      },
    
      "disposition-layout#countdown-site-desktop": {
        "title": "Orden y visualización",
        "children": [
          "rich-text#countdown-site-desktop",
          "countdown#site-desktop",
          "link#countdown-site-desktop"
        ],
        "props": {
          "title": "Contador",
          "disposition": [
            {
              "order": 1,
              "__editorItemTitle": "Texto"
            },
            {
              "order": 2,
              "__editorItemTitle": "Contador"
            },
            {
              "order": 3,
              "__editorItemTitle": "Botón"
            }
          ]
        }
      },
    
      "countdown#site-desktop":{
        "title": "Contador",
        "props":{
          "blockClass": ["countdown", "countdown-site-desktop"]
        }
      },
    
      "link#countdown-site-desktop": {
        "title": "Botón",
        "props": {
          "blockClass": ["countdown", "countdown-site-desktop"],
          "label": "Ver ofertas",
          "href": "/"
        }
      },
    
      "rich-text#countdown-site-desktop": {
        "title": "Texto",
        "props": {
          "blockClass": ["countdown", "countdown-site-desktop"],
          "text": "Lorem Ipsum rem ui"
        }
      },
