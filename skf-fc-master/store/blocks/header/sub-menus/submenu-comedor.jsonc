{
  "vtex.menu@2.x:submenu#comedor": {
    "children": ["flex-layout.row#comedor", "image#comedor-banner"],
    "props": {
      "blockClass": "submenu"
    }
  },

  "flex-layout.row#comedor": {
    "props": {
      "blockClass": "categories-wrapper",
      "colSizing": "auto",
      "colGap": 3
