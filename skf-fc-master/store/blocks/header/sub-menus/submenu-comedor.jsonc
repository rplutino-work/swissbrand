      "vtex.menu@2.x:menu#comedor-muebles-column"
    ]
  },

  /* BANNER MENU */
  "image#comedor-banner": {
    "props": {
      "blockClass": "submenu-banner",
      "src": "https://placehold.jp/430x240.png",
      "link": true
    }
  },

