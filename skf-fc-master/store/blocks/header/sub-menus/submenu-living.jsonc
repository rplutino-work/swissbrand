        {
          "id": "sillones-1-cuerpo",
          "type": "custom",
          "iconId": null,
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/living/sillones/1-cuerpo?fuzzy=0&initialMap=c&initialQuery=living&map=category-1,category-2,cuerpos&operator=and",
            "noFollow": false,
            "text": "1 cuerpo",
            "tagTitle": "1 cuerpo"
          }
        }
      ]
    }
  },

  "menu-item#living-sillones": {
    "props": {
      "blockClass": "category",
