{
  "vtex.menu@2.x:submenu#living": {
    "children": ["flex-layout.row#equipos", "image#equipos-banner"],
    "props": {
      "blockClass": "submenu"
    }
  },

  "flex-layout.row#equipos": {
    "props": {
      "blockClass": "categories-wrapper",
      "colSizing": "auto",
      "colGap": 3
