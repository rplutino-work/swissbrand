      "vtex.menu@2.x:menu#living-rinconeros-column",
      "vtex.menu@2.x:menu#living-muebles-column"
    ]
  },

  /* BANNER MENU */
  "image#equipos-banner": {
    "props": {
      "blockClass": "submenu-banner",
      "src": "https://placehold.jp/430x240.png",
      "link": true
    }
  },

  /* LOCALES COLUMN */
  "vtex.menu@2.x:menu#living-sillones-column": {
    "props": {
      "blockClass": "categories"
    },
    "children": [
      "menu-item#living-sillones",
